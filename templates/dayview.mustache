{{!
    @template local_assessfreq/dayview

    Render the dayview.

    Example context (json):
    {

    }
}}
<div id="local-assessfreq-report-dayview">
<table class="table table-bordered">
  <colgroup>
    <col class="dayview-firstcol">
    <col>
  </colgroup>
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Time</th>
    </tr>
  </thead>
  <tbody>
    {{#rows}}
    <tr>
      <th scope="row">
        <a href="{{url}}}">{{name}}</a> {{# str }} students, local_assessfreq, {{ usercount }} {{/ str }}
      </th>
      <td><div></div></td>
    </tr>
    {{/rows}}

  </tbody>
</table>
</div>
