{{!
    @template local_assessfreq/table

    Table template.

    Example context (json):
    {

    }
}}

<div id="{{table.id}}-table" class="local-assessfreq-card">
    <div class="container-fluid">
        <div class="row justify-content-start card-header">
            <div class="col">{{table.name}}</div>
        </div>
        <div class="row justify-content-between card-contents">
            <div class="container-fluid">
                <div class="row justify-content-between">
                    {{> local_assessfreq/table-filter}}
                </div>
            </div>
            <div class="table-container">
                <span class="overlay-icon-container" data-region="overlay-icon-container">
                    <span class="loading-icon icon-no-margin">{{#pix}} i/loading, core, {{#str}} loading {{/str}} {{/pix}}</span>
                </span>
                <div class="table-body">
                    <!--Table goes here via ajax -->
                </div>
            </div>
        </div>
    </div>
</div>

