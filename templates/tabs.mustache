{{!
    @template local_assessfreq/tabs

    Tab template.
}}

<!-- Tabs -->
<div class="tabs">
{{#reports}}
    <a href="#{{tablink}}" class="tablinks" data-target="tab-{{tablink}}">{{tabname}}</a>
{{/reports}}
{{^reports}}
    {{# str }} noreports, local_assessfreq {{/ str }}
{{/reports}}
</div>

<!-- Tab content -->
{{#reports}}
    <div id="tab-{{tablink}}" class="tabcontent">
        {{{report}}}
    </div>
{{/reports}}

